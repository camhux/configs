{
    "rules": {
        "indent": [
            2,
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-unused-vars": [
            1,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "no-console": 0,
        "space-after-keywords": [2, "always"],
        "space-before-blocks": [2, "always"],
        "space-before-function-paren": [2, "never"],
        "space-in-parens": [2, "never"],
        "no-trailing-spaces": 2,
        "no-native-reassign": 2,
        "quotes": [2, "single"],
        "space-infix-ops": 1,
        "semi": [2, "always"],
        "comma-style": [2, "last"],
        "comma-spacing": [2, {"before": false, "after": true}],
        "no-spaced-func": 2,
        "eol-last": 2,
        "eqeqeq": [2, "allow-null"],
        "radix": 2,
        "wrap-iife": [2, "inside"],
        "spaced-comment": [2, "always"],
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
}